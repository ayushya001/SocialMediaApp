<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view10"
        android:layout_width="@dimen/_40mdp"
        android:layout_height="@dimen/_40mdp"
        android:layout_marginStart="@dimen/_3mdp"
        android:layout_marginTop="@dimen/_3mdp"
        android:background="@drawable/circular_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/vieww"
        android:layout_width="250dp"
        android:layout_height="22dp"
        android:layout_marginStart="24dp"
        android:background="#EaEaEa"
        app:layout_constraintStart_toEndOf="@+id/view10"
        app:layout_constraintTop_toTopOf="@+id/view10" />

    <View
        android:layout_width="180dp"
        android:layout_height="17dp"
        android:layout_marginTop="4dp"
        android:background="#EaEaEa"
        app:layout_constraintStart_toStartOf="@+id/vieww"
        app:layout_constraintTop_toBottomOf="@+id/vieww" />

    <View
        android:id="@+id/viewpic"
        android:layout_width="@dimen/_305mdp"
        android:layout_height="@dimen/_250mdp"
        android:layout_marginTop="4dp"
        android:background="#EaEaEa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view10" />

    <View
        android:id="@+id/viewww"
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:layout_marginTop="4dp"
        android:background="#EaEaEa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewpic" />

    <View
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginTop="4dp"
        android:background="#EaEaEa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewww" />


</androidx.constraintlayout.widget.ConstraintLayout>